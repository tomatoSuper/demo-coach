.reservationTitle2{color:#b8b8b8;font-size:26rpx;margin:10rpx auto;}
.leftReservation view{color:#b8b8b8;font-size:22rpx;}
.leftReservation view text{color:#040a2b;margin-right:15rpx;font-size:22rpx;}
.leftReservation view.appointmentExp{border-top:1px solid #f2f2f2;color:#c6a263;line-height:65rpx;margin:35rpx auto 0;width:100%;text-align: left;letter-spacing: 1px;}
/*预约*/
#red{height:40px;background:red;}
/*会话加载时遮挡提示页*/
.shadowLayer{width:100%;height:100%;background:rgba(0,0,0,.8);position:fixed;left:0;top:0;z-index:9999;}
.shadowLayer image{width:70%;position:absolute;left:15%;top:50%;transform: translate(0,-50%);-webkit-transform:translate(0,-50%); }
/*语音提示*/
.audioGif{width:8%;padding:8%;position:fixed;left:38%;top:50%;transform: translate(0,-50%);-webkit-transform:translate(0,-50%);z-index:9999;background:rgba(0,0,0,.5);border-radius:4px;}
.audioGif image{width:100%;height:auto;}
.audioGif view{font-size:24rpx;color:#fff;text-align: center;display: block;position:absolute;bottom:20rpx;left:0;width:100%;}